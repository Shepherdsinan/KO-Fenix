                    <div class="sidebox_body border_box login-block">
                        @using (Html.BeginForm("Girisyap", "Home", FormMethod.Post))
                        {
                            if (@Session["strAccountID"] != null)
                            {

                                <section class="sidebox_info">
                                    <table width="100%">
                                        <tbody>
                                            <tr>
                                                <td><img src="~/Content/images/icons/user.png"> Kullanıcı Adı</td>
                                                <td id="info_vp">@Session["strAccountID"].ToString()</td>
                                            </tr>
                                            <tr>
                                                <td><img src="~/Content/images/icons/coins.png"> NPoint</td>
                                                <td id="info_vp">0</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <center>
                                        <a href="/Account" class="nice_button">Kullanıcı Paneli</a>

                                        <a href="/Account/Logout" class="nice_button">Çıkış yap</a>
                                    </center>

                                </section>


                            }
                            else
                            {
                                <center>
                                    <p class="login-in"><input type="text" value="" name="strAccountID" id="login_input" placeholder="Kullanıcı Adı" maxlength="10"></p>
                                    <p class="password-in"><input type="password" value="" name="Sifre" id="password_input" placeholder="Parola" maxlength="20"></p>
                                    <div class="login-button flex-center">
                                        <input type="submit" name="login_submit" value="Giriş!">
                                        <a href="Home\ForgotPassword">Şifremi Unuttum ?</a>
                                    </div>
                                </center>
                            }
                        }
                        <section class="sidebox_info" style="display:none"></section>
                    </div>
                </section>
                <script>
                    $(document).ready(function () {

                        $.ajax({
                            url: '/Shared/onlineplayergetir',
                            type: 'POST',
                            dataType: 'json',
                            success: function (data) {
                                $("#onlineplayercount").html(data);
                            }
                        });
                    });
                </script>
                <section id="sidebox_status" class="sidebox">
                    <div class="sidebox_body border_box">
                        <div id="realm_1" class="realm_1 realm_holder">
