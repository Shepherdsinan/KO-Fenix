    <link href="~/Content/css/ucp.css" rel="stylesheet">

    <script>
        var $FileURL = '/Content/';
        var $BaseURL = '/';
        var isIE = false;
    </script>
     


    <script src="~/Content/js/jquery-3.5.1.min.js"></script>
